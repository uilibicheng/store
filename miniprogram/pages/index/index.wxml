<!--pages/index/index.wxml-->
<block>
  <!--头部-->
  <page-header selfStyle="background-color: #ffffff;" />
  <!--banner图-->
  <view bindtap="test" class="page-banner swiper">
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-active-color="#ffffff">
      <block wx:for="{{bannerList}}" wx:key="*this">
        <swiper-item>
          <view class="page-banner-div">
            <image class="page-banner-img" src="{{item.image}}"></image>
          </view>
        </swiper-item>
      </block>
    </swiper>
  </view>
  <!--搜索框-->
  <van-search value="{{ searchValue }}" shape="round" placeholder="请输入商家名称" />
  <!--选择图标-->
  <index-icon programList="{{programList}}" />

  <!--附近商家-->
   <view class="page-business-title">附近商家</view>
   <index-card/> 
  <!--toast-->
   <van-toast id="van-toast" />
</block>