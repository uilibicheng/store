.mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(0, 0, 0, .65);
  z-index: 99999;
}

.container {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  background: #fff;
  padding-bottom: 50rpx;
}

.image {
  display: block;
  margin: 0 auto ;
  width: 542rpx;
  height: 356rpx;
}

.close {
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  width: 32rpx;
  height: 32rpx;
}

.title {
  margin-top: 80rpx;
  font-size: 36rpx;
  font-weight: 500;
  line-height: 42rpx;
  color: #000;
}

.footer {
  margin-top: 58rpx;
}

.btn {
  width: 190rpx;
  height: 72rpx;
  margin: 0 auto;
  background-color: #006CBA;
  font-size: 28rpx;
  line-height: 72rpx;
  border-radius: 72rpx;
  color: #fff;
}
