/* components/contact-entrance/contact-entrance.wxss */
.container {
  width: 88rpx;
  height: 88rpx;
  position: fixed;
  background-image: linear-gradient(90deg, #878585 0%, #616161 100%);
  display: flex;
  align-items: center;
  justify-content: center;
  bottom: 260rpx;
  right: 16rpx;
  z-index: 10;
  border-radius: 50%;
}

.container image {
  width: 48rpx;
  height: 48rpx;
}

.contact {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  opacity: 0;
}
