<!--components/index-icon/index-icon.wxml-->
<view class="icon-container">
  <view wx:for="{{programList}}" wx:key="index" class="icon-container-box" bindtap="handleIcon" data-index="{{index}}">
    <image class="icon-container-img" src="{{item.icon}}"></image>
    <text class="icon-container-text">
          {{item.name}} 
        </text>
    <text wx:if="{{item.corner_content}}" class="icon-container-tag">{{item.corner_content}}</text>
  </view>
</view>