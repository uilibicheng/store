page,
view,
button,
cover-image,
cover-view,
image,
input,
navigator,
scroll-view,
text,
textarea {
  word-wrap: break-word;
  box-sizing: border-box;
  font-family: "PingFang SC", -apple-system, BlinkMacSystemFont, "Helvetica Neue", STHeiti, "Microsoft Yahei", Tahoma, Simsun, sans-serif;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}
page{
  background: #f1f1f1;
}
